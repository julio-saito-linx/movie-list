<div class="imoviesDiv">

  <div class="row">
    <h3>Last Searches</h3>
    <ul class="last_searches col-md-8">
      {{#each searches}}
        <li><a href="/#movies/search/{{query}}">{{query}}</a></li>
      {{/each}}
    </ul>
  </div>

  <div class="row">
    <h3>by Year Ranges</h3>
      <ul>
        {{#each yearRanges}}
          <li class="col-md-4">
            <a href="/#movies/search/year:[{{start}} TO {{end}}]">
              {{start}}..{{end}}
            </a>
          </li>
        {{/each}}
      </ul>
  </div>

  <div class="row">
    <h3>by Year</h3>
    
    {{#each yearsBlock}}
    <div class="row byYear">
      <ul>
  
      {{#each this}}
        <li class="col-md-2"><a href="/#movies/search/year:({{this}})">{{this}}</a></li>
      {{/each}}
  
      </ul>
    </div>
    {{/each}}
  
  </div>

</div>