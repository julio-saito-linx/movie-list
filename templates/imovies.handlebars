<div class="imoviesDiv">

  <div class="row">
    <h3>by Year Ranges</h3>
      <ul>
        {{#each yearRanges}}
          <li class="col-md-2">
            <a href="/#movies/table/search/1/year:[{{start}} TO {{end}}]">
              {{start}}..{{end}}
            </a>
          </li>
        {{/each}}
      </ul>
  </div>

  <div class="row">
    <h3>by Year</h3>
    
    {{#each yearsBlock}}
    <div class="row byYear">
      <ul>
  
      {{#each this}}
        <li class="col-md-1"><a href="/#movies/table/search/1/year:({{this}})">{{this}}</a></li>
      {{/each}}
  
      </ul>
    </div>
    {{/each}}
  
  </div>

</div>