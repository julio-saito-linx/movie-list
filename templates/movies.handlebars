<div id="search-result" class="col-md-12">
  <div class="row">
    <div class="col-md-2">
      <h1>Movies</h1>
    </div>
    <div class="col-md-3">
      <button class="btnShowThumbs">show thumbs</button>
    </div>
  </div>
  
  <table class="table table-bordered table-hover table table-striped">
    <thead>
      <tr>
        <th class='rating'>
          rating
        </th>
        <th class='year'>
          year
        </th>
        <th>
          title
        </th>
        <th>
          pt-BR title
        </th>
        <th>
          director
        </th>
        <th>
          imdb
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each results}}
        <tr class="trMovie" data-id="{{id}}">
          <td class='rating'>{{imdbInfo.rating}}</td>
          <td class='year'>{{year}}</td>
          <td>{{titleAndOriginal}}</td>
          <td>{{imdbInfo.title_ptbr}}</td>
          <td>{{director}}</td>
          <td><a target="blank" href="http://www.imdb.com/title/{{idImdb}}/">{{idImdb}}</a></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>